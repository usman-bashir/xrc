<?xml version="1.0" encoding="utf-8" ?>
<xrc:page xmlns:xrc="urn:xrc"
         xmlns:GoogleNews="xrc:GoogleNewsModule">

	<xrc:parameters>
		<xrc:add key="value" allowRequestOverride="true" />
	</xrc:parameters>

	<xrc:action method="GET" layout="">
		<xrc:JsonView>
			<Content>@GoogleNews.Search(value)</Content>
		</xrc:JsonView>
	</xrc:action>

</xrc:page>
