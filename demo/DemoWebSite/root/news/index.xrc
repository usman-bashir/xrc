<?xml version="1.0" encoding="utf-8" ?>
<xrc:page xmlns:xrc="urn:xrc"
         xmlns:File="xrc:FileModule">

	<xrc:parameters>
		<xrc:add key="title" value="News" />
		<xrc:add key="activeMenu" value="news" />
	</xrc:parameters>

	<xrc:action method="GET" parent="~/shared/_layout">
		<xrc:XHtmlView>
			<Content>@File.Xml("index.xhtml")</Content>
		</xrc:XHtmlView>
	</xrc:action>

</xrc:page>
