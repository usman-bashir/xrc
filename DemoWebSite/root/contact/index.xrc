<?xml version="1.0" encoding="utf-8" ?>
<xrc:xrc xmlns:xrc="urn:xrc"
         xmlns:Player="xrc:PlayerModule">

  <xrc:parameters>
    <xrc:add key="title" value="Contact" />
    <xrc:add key="param1" value="@DateTime.Now" type="System.DateTime" />
  </xrc:parameters>

  <xrc:action method="GET" parent="~/shared/_layout">
		<xrc:renderer type="RazorRenderer">
			<view>index.cshtml</view>
			<model>@Player.GetPlayer()</model>
		</xrc:renderer>
	</xrc:action>
  
</xrc:xrc>
