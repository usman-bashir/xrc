<?xml version="1.0" encoding="utf-8" ?>
<xrc:page xmlns:xrc="urn:xrc"
         xmlns:File="xrc:FileModule">

	<xrc:parameters>
		<!-- This parameter will be used as the page title in the shared/layout.xslt page -->
		<xrc:add key="title" value="actions test" />
		<!-- This parameter will be passed on the shared/_header.xrc -->
